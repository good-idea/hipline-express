{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all dev:js && npm-run-all --parallel dev:css livereload serve",
    "dev:js": "webpack --progress --colors",
    "dev:css": "./node_modules/.bin/postcss src/css/*.css --dir public/css/ -c postcss.config.js -w",
    "build": "npm-run-all build:js build:css",
    "build:js": "webpack -p --config webpack.config.production.js --progress --colors",
    "build:css": "./node_modules/.bin/postcss src/css/*.css --dir public/css/ -c postcss.config.js",
    "livereload": "livereload 'public/css'",
    "serve": "nodemon server.js --ignore public/ --ignore src/"
  },
